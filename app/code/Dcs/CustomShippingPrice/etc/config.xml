        <dcs_customshippingprice>
            <general>
                <module_name>Digital Shipping Method</module_name> 
            </general>
        </dcs_customshippingprice>
        <carriers>
            <dcsshipping>
                <active>1</active>
                <sallowspecific>0</sallowspecific>

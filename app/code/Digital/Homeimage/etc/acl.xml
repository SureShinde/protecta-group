<?xml version="1.0"?> 
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
<acl>
	<resources>
		<resource id="Magento_Backend::admin">
			<resource id="Digital_Homeimage::parent" title="Homeimage" sortOrder="100" >
				<resource id="Digital_Homeimage::homeimage" title="Manage Homeimage" sortOrder="0"/>
                <resource id="Digital_Homeimage::settings" title="Settings" sortOrder="20"/>
            </resource>
			<resource id="Magento_Backend::stores">
				<resource id="Magento_Backend::stores_settings">
					<resource id="Magento_Config::config">
						<!-- this resource id we can use in system.xml for section -->
						<resource id="Digital_Homeimage::config_image" title="Homeimage Section" sortOrder="80" />
					</resource>
				</resource>
			</resource>
		</resource>
	</resources>
</acl>
</config>